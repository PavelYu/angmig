{
    "name": "angmig",
    "version": "0.0.0",
    "scripts": {
        "start": "ng serve",
        "build": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng build --configuration=production",
        "build:stats": "npm run build --stats-json",
        "analyze": "webpack-bundle-analyzer ./dist/msp-multisnap/stats.json",
        "test": "ng test --code-coverage",
        "test:watch": "ng test --watch=true",
        "test:ci": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng test --watch=false --browsers=ChromeHeadlessNoSandbox --code-coverage",
        "generate:svg-sprite": "svg2sprite ./src/styles/icons ./src/assets/msp-icons.svg --stripAttrs stroke",
        "generate:svg-sprite_flag": "svg2sprite ./src/assets/flags/min ./src/assets/flags/flags.svg --stripAttrs stroke",
        "generate:svg-sprite_perils": "svg2sprite ./src/assets/Perils ./src/assets/perils.svg --stripAttrs stroke",
        "svgo": "svgo -f src/assets/flags/raw -o src/assets/flags/min --precision=0 --enable=removeRasterImages,sortAttrs,removeStyleElement,removeScriptElement,prefixIds",
        "lint": "eslint \"src/**/*.ts\"",
        "lint:errors": "eslint --quiet \"src/**/*.ts\"",
        "csscomb": "node csscomb",
        "prepare": "husky"
    },
    "engines": {
        "node": "18 || 20 || 22",
        "yarn": "Please use NPM instead of yarn to install dependencies",
        "npm": "10"
    },
    "browserslist": [
        "last 2 Chrome versions"
    ],
    "dependencies": {
        "@ag-grid-community/angular": "~28.2.1",
        "@ag-grid-community/client-side-row-model": "~28.2.1",
        "@ag-grid-community/core": "~28.2.1",
        "@ag-grid-enterprise/clipboard": "~28.2.1",
        "@ag-grid-enterprise/core": "~28.2.1",
        "@ag-grid-enterprise/excel-export": "~28.2.1",
        "@ag-grid-enterprise/master-detail": "~28.2.1",
        "@ag-grid-enterprise/menu": "~28.2.1",
        "@ag-grid-enterprise/range-selection": "~28.2.1",
        "@ag-grid-enterprise/row-grouping": "~28.2.1",
        "@angular/animations": "^15.2.10",
        "@angular/cdk": "^15.2.9",
        "@angular/common": "^15.2.10",
        "@angular/compiler": "^15.2.10",
        "@angular/core": "^15.2.10",
        "@angular/forms": "^15.2.10",
        "@angular/language-service": "^15.2.10",
        "@angular/material": "^15.2.9",
        "@angular/material-moment-adapter": "^15.2.9",
        "@angular/platform-browser": "^15.2.10",
        "@angular/platform-browser-dynamic": "^15.2.10",
        "@angular/router": "^15.2.10",
        "@highcharts/map-collection": "2.1.0",
        "@mdi/font": "^7.4.47",
        "@ngx-translate/core": "14.0.0",
        "@ngx-translate/http-loader": "7.0.0",
        "@swimlane/ngx-graph": "8.0.2",
        "@types/dagre": "0.7.52",
        "d3-scale": "^4.0.2",
        "d3-shape": "3.2.0",
        "highcharts": "9.3.3",
        "highcharts-angular": "3.1.2",
        "http-status-codes": "2.3.0",
        "lodash": "~4.17.21",
        "marked": "12.0.1",
        "material-design-icons": "~3.0.1",
        "moment": "~2.29.1",
        "moment-timezone": "0.5.28",
        "ng-in-viewport": "^6.1.5",
        "ngx-infinite-scroll": "14.0.1",
        "ngx-material-timepicker": "12.1.0",
        "ngx-perfect-scrollbar": "~10.1.1",
        "proj4": "2.11.0",
        "rxjs": "~7.5.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^15.2.11",
        "@angular/cli": "^15.2.11",
        "@angular/compiler-cli": "^15.2.10",
        "@types/jasmine": "~4.0.0",
        "@types/jasminewd2": "~2.0.13",
        "@types/node": "18.19.31",
        "@types/resize-observer-browser": "~0.1.11",
        "@typescript-eslint/eslint-plugin": "6.21.0",
        "@typescript-eslint/parser": "6.21.0",
        "csscomb": "^4.3.0",
        "eslint": "7.32.0",
        "husky": "9.0.11",
        "jasmine-core": "~4.3.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "~3.1.1",
        "karma-coverage": "2.2.1",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.0.0",
        "karma-junit-reporter": "2.0.1",
        "karma-spec-reporter": "0.0.36",
        "svg2sprite-cli": "2.0.1",
        "svgo": "^1.3.2",
        "typescript": "~4.9.0",
        "webpack-bundle-analyzer": "~4.5.0"
    }
}
